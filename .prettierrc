{
  "printWidth": 100,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "all",
  "bracketSpacing": true,
  "arrowParens": "always",
  "endOfLine": "crlf",
  "jsxSingleQuote": true,
  "bracketSameLine": false,
  "quoteProps": "as-needed",
  "plugins": ["prettier-plugin-tailwindcss"]
}
